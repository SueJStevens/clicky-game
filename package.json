{
  "name": "clicky-game",
  "version": "1.0.0",
  "description": "Memory Game to Demo React Components, State, and Response to User Events Bootcamp Assignment 1/12/2019 - All the News That's Fit to Scrape",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SueJStevens/clicky-game.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SueJStevens/clicky-game/issues"
  },
  "homepage": "https://github.com/SueJStevens/clicky-game#readme"
}
